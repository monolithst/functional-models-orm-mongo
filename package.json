{
  "name": "functional-models-orm-mongo",
  "version": "1.0.12",
  "description": "An ORM for functional-models that uses mongodb.",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "npm run build && nyc mocha --recursive --full-trace --require source-map-support/register ./test/**/*.test.js",
    "feature-tests": "./node_modules/@cucumber/cucumber/bin/cucumber-js",
    "coverage": "npm run build && nyc --all --reporter=lcov npm test",
    "build": "rm -Rf ./dist && mkdir -p ./dist && cp -R ./src/* ./dist &&  cd ./dist && tsc -p ../tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monolithst/functional-models-orm-mongo.git"
  },
  "keywords": [
    "orm",
    "mongodb",
    "nodejs",
    "functional"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/monolithst/functional-models-orm-mongo/issues"
  },
  "author": "Mike Cornwell",
  "dependencies": {
    "functional-models": "git@github.com:monolithst/functional-models.git#1.0",
    "functional-models-orm": "git@github.com:monolithst/functional-models.git#1.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.3"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "dist/*.ts",
      "dist/*.js",
      "dist/**/*.ts",
      "dist/**/*.js"
    ],
    "cache": true,
    "extension": [
      ".ts"
    ],
    "require": [
      "source-map-support/register"
    ],
    "exclude": [
      "node_modules",
      "src/",
      "src/*.d.ts",
      "test/*"
    ],
    "instrument": true,
    "sourceMap": true
  },
  "devDependencies": {
    "@types/lodash": "^4.14.177",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.0",
    "cucumber": "^7.0.0-rc.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^11.1.2"
  },
  "homepage": "https://github.com/monolithst/functional-models-orm-mongo#readme"
}
